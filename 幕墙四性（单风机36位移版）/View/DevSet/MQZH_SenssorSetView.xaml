                            <ComboBox ItemsSource="{Binding Dev.ElecSigUnitList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[1].ElecSigUnit, Mode=TwoWay}" Grid.Row="2" Grid.Column ="2" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.ElecSigUnitList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[2].ElecSigUnit, Mode=TwoWay}" Grid.Row="3" Grid.Column ="2" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.ElecSigUnitList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[3].ElecSigUnit, Mode=TwoWay}" Grid.Row="4" Grid.Column ="2" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>

                            <Label Content="电信号下限" Grid.Row ="0" Grid.Column="3" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[0].ElecSigLowerRange}" Grid.Row ="1" Grid.Column="3" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[1].ElecSigLowerRange}" Grid.Row ="2" Grid.Column="3" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[2].ElecSigLowerRange}" Grid.Row ="3" Grid.Column="3" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[3].ElecSigLowerRange}" Grid.Row ="4" Grid.Column="3" Style="{StaticResource TextBox1}" />

                            <Label Content="电信号上限" Grid.Row ="0" Grid.Column="4" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[0].ElecSigUpperRange}" Grid.Row ="1" Grid.Column="4" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[1].ElecSigUpperRange}" Grid.Row ="2" Grid.Column="4" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[2].ElecSigUpperRange}" Grid.Row ="3" Grid.Column="4" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[3].ElecSigUpperRange}" Grid.Row ="4" Grid.Column="4" Style="{StaticResource TextBox1}" />

                            <Label Content="采集值下限" Grid.Row ="0" Grid.Column="5" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[0].DataLowerRange}" Grid.Row ="1" Grid.Column="5" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[1].DataLowerRange}" Grid.Row ="2" Grid.Column="5" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[2].DataLowerRange}" Grid.Row ="3" Grid.Column="5" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[3].DataLowerRange}" Grid.Row ="4" Grid.Column="5" Style="{StaticResource TextBox1}" />

                            <Label Content="采集值上限" Grid.Row ="0" Grid.Column="6" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[0].DataUpperRange}" Grid.Row ="1" Grid.Column="6" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[1].DataUpperRange}" Grid.Row ="2" Grid.Column="6" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[2].DataUpperRange}" Grid.Row ="3" Grid.Column="6" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[3].DataUpperRange}" Grid.Row ="4" Grid.Column="6" Style="{StaticResource TextBox1}" />

                            <Label Content="滤波系数" Grid.Row ="0" Grid.Column="7" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[0].FitRatio}" Grid.Row ="1" Grid.Column="7" Style="{StaticResource TextBox1}" ToolTip ="滤波系数，1-50"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[1].FitRatio}" Grid.Row ="2" Grid.Column="7" Style="{StaticResource TextBox1}" ToolTip ="滤波系数，1-50"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[2].FitRatio}" Grid.Row ="3" Grid.Column="7" Style="{StaticResource TextBox1}" ToolTip ="滤波系数，1-50"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[3].FitRatio}" Grid.Row ="4" Grid.Column="7" Style="{StaticResource TextBox1}" ToolTip ="滤波系数，1-50"/>

                            <Label Content="占用标志" Grid.Row ="0" Grid.Column="8" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD1.Channels[0].IsUsed}" IsEnabled="False " Grid.Row="1" Grid.Column="8" />
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD1.Channels[1].IsUsed}" IsEnabled="False " Grid.Row="2" Grid.Column="8" />
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD1.Channels[2].IsUsed}" IsEnabled="False " Grid.Row="3" Grid.Column="8" />
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD1.Channels[3].IsUsed}" IsEnabled="False " Grid.Row="4" Grid.Column="8" />
                        </Grid>
                    </GroupBox>

                    <GroupBox Header="AI通道-AD模块2（位移尺5-8）" Grid.Row ="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="5" BorderBrush="Black" FontSize="16">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
