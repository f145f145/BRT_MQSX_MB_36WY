                            <Label Content="电信号下限" Grid.Row ="0" Grid.Column="3" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[0].ElecSigLowerRange}" Grid.Row ="1" Grid.Column="3" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[1].ElecSigLowerRange}" Grid.Row ="2" Grid.Column="3" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[2].ElecSigLowerRange}" Grid.Row ="3" Grid.Column="3" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[3].ElecSigLowerRange}" Grid.Row ="4" Grid.Column="3" Style="{StaticResource TextBox1}" />

                            <Label Content="电信号上限" Grid.Row ="0" Grid.Column="4" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[0].ElecSigUpperRange}" Grid.Row ="1" Grid.Column="4" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[1].ElecSigUpperRange}" Grid.Row ="2" Grid.Column="4" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[2].ElecSigUpperRange}" Grid.Row ="3" Grid.Column="4" Style="{StaticResource TextBox1}" />
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[3].ElecSigUpperRange}" Grid.Row ="4" Grid.Column="4" Style="{StaticResource TextBox1}" />

                            <Label Content="采集值下限" Grid.Row ="0" Grid.Column="5" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD2.Channels[0].DataLowerRange}" Grid.Row ="1" Grid.Column="5" Style="{StaticResource TextBox1}" />
