                            <Label Content="接口类型" Grid.Row ="0" Grid.Column="1" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[0].InfType, Mode=TwoWay}" Grid.Row="1" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[1].InfType, Mode=TwoWay}" Grid.Row="2" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[2].InfType, Mode=TwoWay}" Grid.Row="3" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[3].InfType, Mode=TwoWay}" Grid.Row="4" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False" />

                            <Label Content="电信号单位" Grid.Row ="0" Grid.Column="2" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <ComboBox ItemsSource="{Binding Dev.ElecSigUnitList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[0].ElecSigUnit, Mode=TwoWay}" Grid.Row="1" Grid.Column ="2" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.ElecSigUnitList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[1].ElecSigUnit, Mode=TwoWay}" Grid.Row="2" Grid.Column ="2" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.ElecSigUnitList}" SelectedItem ="{Binding Dev .Mod_AD2.Channels[2].ElecSigUnit, Mode=TwoWay}" Grid.Row="3" Grid.Column ="2" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
