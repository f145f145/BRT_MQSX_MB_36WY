                            <Label Content="占用标志" Grid.Row ="0" Grid.Column="8" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD2.Channels[0].IsUsed}" IsEnabled="False " Grid.Row="1" Grid.Column="8" />
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD2.Channels[1].IsUsed}" IsEnabled="False " Grid.Row="2" Grid.Column="8" />
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD2.Channels[2].IsUsed}" IsEnabled="False " Grid.Row="3" Grid.Column="8" />
                            <CheckBox Content="已占用" IsChecked ="{Binding Dev.Mod_AD2.Channels[3].IsUsed}" IsEnabled="False " Grid.Row="4" Grid.Column="8" />
                        </Grid>
                    </GroupBox>

                    <GroupBox Header="AI通道-AD模块3（位移尺9-12）" Grid.Row ="2" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="5" BorderBrush="Black" FontSize="16">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Label Content="通道编号" Grid.Row ="0" Grid.Column="0" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD3.Channels[0].ChannelNO}" Grid.Row ="1" Grid.Column="0" Style="{StaticResource TextBox1}" IsEnabled ="False"/>
                            <TextBox Text="{Binding Dev.Mod_AD3.Channels[1].ChannelNO}" Grid.Row ="2" Grid.Column="0" Style="{StaticResource TextBox1}" IsEnabled ="False"/>
