                            <Grid.RowDefinitions>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                                <RowDefinition Height=" *"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Label Content="通道编号" Grid.Row ="0" Grid.Column="0" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[0].ChannelNO}" Grid.Row ="1" Grid.Column="0" Style="{StaticResource TextBox1}" IsEnabled ="False"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[1].ChannelNO}" Grid.Row ="2" Grid.Column="0" Style="{StaticResource TextBox1}" IsEnabled ="False"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[2].ChannelNO}" Grid.Row ="3" Grid.Column="0" Style="{StaticResource TextBox1}" IsEnabled ="False"/>
                            <TextBox Text="{Binding Dev.Mod_AD1.Channels[3].ChannelNO}" Grid.Row ="4" Grid.Column="0" Style="{StaticResource TextBox1}" IsEnabled ="False"/>

                            <Label Content="接口类型" Grid.Row ="0" Grid.Column="1" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[0].InfType, Mode=TwoWay}" Grid.Row="1" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[1].InfType, Mode=TwoWay}" Grid.Row="2" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[2].InfType, Mode=TwoWay}" Grid.Row="3" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_AD1.Channels[3].InfType, Mode=TwoWay}" Grid.Row="4" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False" />

